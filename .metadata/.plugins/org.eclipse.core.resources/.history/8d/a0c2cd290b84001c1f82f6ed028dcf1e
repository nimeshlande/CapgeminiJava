package Capgemini.com;

public class FloatMethods {
	public static void main(String[] args) {

		float a = 5.55F;

		String Parsenum = "a";

		// toString Method
		System.out.println("toString(a) = " + Float.toString(a));

		// parseFloat
		float b = Float.parseFloat(Parsenum);
		System.out.println("parseFloat(b) = " + b);
	}

}
